
pipeline {
    agent any
    
    WithCredentials([[
       credentialsId: 'sshssh'
       ]])

   

    stages {
        stage('ssh') {
            steps {
                sshagent (credentials: ['sshssh']) {
                    sh 'ssh nelsonbilly@69.248.2.173'
                }
            }
        }
      }  
    }
   
               
         
          
     

