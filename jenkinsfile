
pipeline {
    agent any
    
    environment {
        USER_CREDENTIALS = credentials('mycredentials')
    }
 
    stages {
        stage('ssh') {
            steps {
               withCredentials([
                   usernamePassword(credentialsId: mycredentials, usernameVariable: 'USER', passwordVariable: 'PASS')
                /*sshagent (credentials: ['mycredentials']) {*/
                     sh 'ssh coni@172.31.5.142'
                    /*sh 'ssh nelsonbilly@69.248.2.173'*/
                          
                      git branch: 'master', credentialsId: 'mycredentials', url: 'https://github.com/billyogendo/go-ecs-ecr.git'
                }
              
            }
        }
      }  
    }
   
               
         
          
     

